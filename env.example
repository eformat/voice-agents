# API/Model configuration
# Copy to .env and fill in secrets locally (avoid committing real keys).

# Supervisor model
BASE_URL="https://your-endpoint.example.com/v1"
MODEL_NAME="your-model-name"
# pragma: allowlist nextline secret
API_KEY="your-api-key"

# Text to speech model
TTS_URL="https://your-tts-model-endpoint.example.com/v1/audio/speech"
TTS_MODEL="higgs-audio-v2-generation-3B-base"
TTS_VOICE="belinda"

# Speech to text model
STT_URL="https://your-stt-model-endpoint.example.com/v1/audio/transcriptions"
STT_MODEL="whisper"
# pragma: allowlist nextline secret
STT_TOKEN="your-bearer-token-if-required"
